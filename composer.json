{
    "name": "trangfoo/hprose-lumen",
    "description": "Hprose for Lumen",
    "keywords": ["hprose", "lumen"],
    "license": "MIT",
    "authors": [
        {
            "name": "Frank.Luo",
            "email": "manqiang.luo@gamil.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "hprose/hprose": "v2.0.*",
        "laravelista/lumen-vendor-publish": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Trangfoo\\HproseLumen\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trangfoo\\HproseLumen\\ServiceProvider"
            ],
            "aliases": {
                "LaravelHproseRouter": "Trangfoo\\HproseLumen\\Facades\\Router"
            }
        }
    },
    "scripts": {
        "post-install-cmd": [
            "find vendor -type d -name .git -exec rm -rf '{}' \\;"
        ],
        "post-update-cmd": [
            "find vendor -type d -name .git -exec rm -rf '{}' \\;"
        ]
    }
}
